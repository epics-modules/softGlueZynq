record(bo, "$(P)$(Q)acquireDmaEnable") {
	field(ZNAM, "Disable")
	field(ONAM, "Enable")
}
record(mbbo, "$(P)$(Q)acquireDmaMode") {
	field(NOBT,"2")
	field(ZRST, "2D")
	field(ONST, "List")
	field(TWST, "x,y,t")
	field(THST, "MCS")
}
record(aSub, "$(P)$(Q)acquireDma") {
  field(SDIS, "$(P)$(Q)acquireDmaEnable")
  field(DISV, 0)
  field(INAM, "acquireDma_init")
  field(SNAM, "acquireDma_do")
  field(PREC, "5")
  field(FTA, "LONG")
  field(FTB, "LONG")
  field(FTC, "LONG")
  field(FTD, "LONG")
  field(FTE, "DOUBLE")
  field(FTF, "LONG")
  field(FTG, "LONG")
  field(INPG, "$(P)$(Q)acquireDmaMode")
  field(FTH, "LONG")

  # scale factors
  field(FTI, "FLOAT")
  field(INPI, "1")
  field(FTJ, "FLOAT")
  field(INPJ, "1")
  field(FTK, "FLOAT")
  field(INPK, "1")
  field(FTL, "FLOAT")
  field(INPL, "1")
  field(FTM, "FLOAT")
  field(INPM, "1")
  field(FTN, "FLOAT")
  field(INPN, "1")
  field(FTO, "FLOAT")
  field(INPO, "1")
  field(FTP, "FLOAT")

  field(FTVA, "FLOAT")
  field(NOVA, "$(N)")
  field(FTVB, "FLOAT")
  field(NOVB, "$(N)")
  field(FTVC, "FLOAT")
  field(NOVC, "$(N)")
  field(FTVD, "FLOAT")
  field(NOVD, "$(N)")
  field(FTVE, "FLOAT")
  field(NOVE, "$(N)")
  field(FTVF, "FLOAT")
  field(NOVF, "$(N)")
  field(FTVG, "FLOAT")
  field(NOVG, "$(N)")
  field(FTVH, "FLOAT")
  field(NOVH, "$(N)")

# recorded events
  field(FTVI, "LONG")
  field(NOVI, "1")
  
# cirBufWords
  field(FTVJ, "LONG")
  field(NOVJ, "1")
  }
