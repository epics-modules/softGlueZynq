{In_23 $(PORT2) $(ADDR2=0x800010) 0x000c0}
{In_24 $(PORT2) $(ADDR2=0x800010) 0x00180}
{In_25 $(PORT2) $(ADDR2=0x800010) 0x00300}
{In_26 $(PORT2) $(ADDR2=0x800010) 0x00600}
{In_27 $(PORT2) $(ADDR2=0x800010) 0x00c00}
{In_28 $(PORT2) $(ADDR2=0x800010) 0x01800}
{In_29 $(PORT2) $(ADDR2=0x800010) 0x03000}
{In_30 $(PORT2) $(ADDR2=0x800010) 0x06000}
{In_31 $(PORT2) $(ADDR2=0x800010) 0x0c000}
{In_32 $(PORT2) $(ADDR2=0x800010) 0x18000}

{In_33 $(PORT3) $(ADDR3=0x800020) 0x00003}
{In_34 $(PORT3) $(ADDR3=0x800020) 0x00006}
{In_35 $(PORT3) $(ADDR3=0x800020) 0x0000c}
{In_36 $(PORT3) $(ADDR3=0x800020) 0x00018}
{In_37 $(PORT3) $(ADDR3=0x800020) 0x00030}
{In_38 $(PORT3) $(ADDR3=0x800020) 0x00060}
{In_39 $(PORT3) $(ADDR3=0x800020) 0x000c0}
{In_40 $(PORT3) $(ADDR3=0x800020) 0x00180}
{In_41 $(PORT3) $(ADDR3=0x800020) 0x00300}
{In_42 $(PORT3) $(ADDR3=0x800020) 0x00600}
{In_43 $(PORT3) $(ADDR3=0x800020) 0x00c00}
{In_44 $(PORT3) $(ADDR3=0x800020) 0x01800}
{In_45 $(PORT3) $(ADDR3=0x800020) 0x03000}
{In_46 $(PORT3) $(ADDR3=0x800020) 0x06000}
{In_47 $(PORT3) $(ADDR3=0x800020) 0x0c000}
{In_48 $(PORT3) $(ADDR3=0x800020) 0x18000}

